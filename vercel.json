{
    "version": 2,
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.7.1"
      }
    },
    "routes": [
      {
        "src": "/build/(.*)",
        "dest": "/public/build/$1"
      },
      {
        "src": "/favicon.png",
        "dest": "/public/favicon.png"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ]
  }